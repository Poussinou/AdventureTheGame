sub keys (k) {
	if (k == " ") showBoxes ();
	if (k == "f") showFloor ();
	if (k == "ESCAPE") quitGame ();
	
	if (k == "1") gotoRoom(room01);
	if (k == "2") gotoRoom(roomElevator);
	if (k == "3") gotoRoom(room02);
	if (k == "4") gotoRoom(room03);
	if (k == "5") gotoRoom(room04);
	if (k == "6") gotoRoom(room05);
	if (k == "7") gotoRoom(room06);
	
	if (k == "i") showInventory();
	
	if(k == "c") pushToStack(inventory, item_gumWhole);
	
	if (k == "a") narratorSay("Abc");
}
